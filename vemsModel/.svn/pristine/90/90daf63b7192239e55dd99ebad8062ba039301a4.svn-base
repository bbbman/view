    <script type="text/javascript">
    $(function(){
        if (!$.browser.webkit) {
            $.mCustomScrollbar.defaults.scrollButtons.enable=false; //滑块的上下的button是否显示
            $.mCustomScrollbar.defaults.axis="y"; //滑块的放行 X轴或Y轴
             $(".main-sidebar").mCustomScrollbar({theme:"dark"});
        }

    });

    $("#addCoupon").click(function(){
        layer.open({
            type: 2,
            title: '新增本地优惠券',
            maxmin: true,
            shadeClose: false, //点击遮罩关闭层
            area : ['68%' , '71.5%'],
            offset : ['30px'],
            content: ['modal/addLocalCouponModal.html']
        });
        var wrap =  $(".layui-layer-content ");
        browser_ = /iphone|ipad/i.test(navigator.userAgent);
        if(browser_ == true) {
            wrap.addClass("iframe-wrap");
        }
        return false;
    })
    $(".modifyCoupon").click(function(){
        layer.open({
            type: 2,
            title: '修改本地优惠券',
            maxmin: true,
            shadeClose: false, //点击遮罩关闭层
            area : ['68%' , '71.5%'],
            offset : ['30px'],
            content: ['modal/modifyLocalCouponModal.html']
        });
        var wrap =  $(".layui-layer-content ");
        browser_ = /iphone|ipad/i.test(navigator.userAgent);
        if(browser_ == true) {
            wrap.addClass("iframe-wrap");
        }
        return false;
    });

    
    $(document).on("click", ".localCouponTongbuDetail", function() {
        layer.open({
            type: 2,
            title: '停车场同步详情',
            maxmin: true,
            shadeClose: false, //点击遮罩关闭层
            area: ['1000px', '80%'],
            offset: ['30px'],
            content: ['modal/localCouponTongbuDetail-modal.html']
        });
        var wrap = $(".layui-layer-content ");
        browser_ = /iphone|ipad/i.test(navigator.userAgent);
        if (browser_ == true) {
            wrap.addClass("iframe-wrap");
        }
        var wrap = $(".layui-layer-content ");
        browser_ = /iphone|ipad/i.test(navigator.userAgent);
        if (browser_ == true) {
            wrap.addClass("iframe-wrap");
        }
        return false;
    });
        
