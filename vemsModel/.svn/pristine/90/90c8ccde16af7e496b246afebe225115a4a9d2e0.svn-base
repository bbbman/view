.ake-keyboard-wrap {
  position: fixed;
  bottom: 10px;
  left: 50%;
  margin-left: -397px;
  z-index: 9999;
  background-color: #fff;
  border: 10px solid #337ab7;
  border-radius: 8px;
  padding:18px 20px 2px 40px;
  height: 450px;
  width: 795px;
  -webkit-box-shadow: 0px 0px 15px rgba(0, 0, 0, 0.48);
  box-shadow: 0px 0px 15px rgba(0, 0, 0, 0.48);
}
.ake-keyboard-wrap .ake-keyboard-box ul {
  margin-bottom: 10px;
}
.ake-keyboard-wrap .ake-keyboard-box li {
  width: 45px;
  height: 45px;
  color: #fff;
  text-align: center;
  vertical-align: middle;
  line-height: 45px;
  font-size: 18px;
  font-weight: bold;
  cursor: pointer;
  margin: 0 10px 10px 0;
  background: url('../images/button_key.png') no-repeat center;
  background-size: 45px 45px;
}
.ake-keyboard-wrap .ake-keyboard-box li:hover {
  background-image: url("../images/button_key_pressed.png");
}
.ake-keyboard-wrap .ake-keyboard-box .backspace {
  width: 45px;
  height: 45px;
  background: url('../images/button_del.png') no-repeat center;
  background-size: 45px 45px;
}
.ake-keyboard-wrap .ake-keyboard-box .backspace:hover {
  background-image: url("../images/button_del_pressed.png");
}
.ake-keyboard-wrap .ake-keyboard-box .enter {
  width: 100px;
  height: 45px;
  background: url('../images/button_clear.png') no-repeat center;
  background-size: 100px 45px;
  text-align: left;
  padding-left: 15px;
}
.ake-keyboard-wrap .ake-keyboard-box .enter:hover {
  background-image: url("../images/button_clear_pressed.png");
}
.ake-keyboard-footer .keyboard-big-btn {
  width: 100px;
  height: 45px;
  background: url('../images/button_ake.png') no-repeat center;
  background-size: 100px 45px;
  text-align: center;
  margin: 0px 13px 0 0;
  color: #fff;
  font-weight: bold;
  font-size: 18px;
  line-height: 45px;
  cursor: pointer;
  text-decoration:none;
}
.ake-keyboard-footer .keyboard-big-btn:hover {
  background-image: url("../images/button_ake_pressed.png");
}