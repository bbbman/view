}
.data-part .form-group .checkbox-inline input[type=radio],
#clearTrashDataModal .form-group .checkbox-inline input[type=radio],
#clearHistoryDataModal .form-group .checkbox-inline input[type=radio],
.newInputStyle .form-group .checkbox-inline input[type=radio],
.newInputStyle .form-group .radio-inline input[type=radio] {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  width: 20px;
  height: 20px;
  margin: 0;
  cursor: pointer;
  vertical-align: bottom;
  background: #FFF;
  border: 3px solid #979797;
  border-radius: 20px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  position: relative!important;
  right: 3px;
}
.data-part .form-group .checkbox-inline input[type=radio]:active,
#clearTrashDataModal .form-group .checkbox-inline input[type=radio]:active,
#clearHistoryDataModal .form-group .checkbox-inline input[type=radio]:active,
.newInputStyle .form-group .checkbox-inline input[type=radio]:active,
.newInputStyle .form-group .radio-inline input[type=radio]:active {
  background: #FFF;
  border: 6px solid #979797;
}
.data-part .form-group .checkbox-inline input[type=radio]:checked,
#clearTrashDataModal .form-group .checkbox-inline input[type=radio]:checked,
#clearHistoryDataModal .form-group .checkbox-inline input[type=radio]:checked,
.newInputStyle .form-group .checkbox-inline input[type=radio]:checked,
.newInputStyle .form-group .radio-inline input[type=radio]:checked {
  background: #FFF;
  border: 6px solid #979797;
}
.data-part .form-group .checkbox-inline input[type=checkbox]:active,
#clearTrashDataModal .form-group .checkbox-inline input[type=checkbox]:active,
#clearHistoryDataModal .form-group .checkbox-inline input[type=checkbox]:active,
.newInputStyle .form-group .checkbox-inline input[type=checkbox]:active,
.newInputStyle .form-group .radio-inline input[type=checkbox]:active {
  background: #979797;
  border: 0;
}
.data-part .form-group .checkbox-inline input[type=checkbox]:checked,
#clearTrashDataModal .form-group .checkbox-inline input[type=checkbox]:checked,
#clearHistoryDataModal .form-group .checkbox-inline input[type=checkbox]:checked,
.newInputStyle .form-group .checkbox-inline input[type=checkbox]:checked,
.newInputStyle .form-group .radio-inline input[type=checkbox]:checked {
  background: #979797;
  border: 0;
}
.data-part .form-group .checkbox-inline input[type=checkbox]:checked::after,
#clearTrashDataModal .form-group .checkbox-inline input[type=checkbox]:checked::after,
#clearHistoryDataModal .form-group .checkbox-inline input[type=checkbox]:checked::after,
.newInputStyle .form-group .checkbox-inline input[type=checkbox]:checked::after,
.newInputStyle .form-group .radio-inline input[type=checkbox]:checked::after {
  content: url(../images/ok.png);
  background-position: cover;
  display: block;
  position: absolute;
  top: -4px;
  right: 0px;
  left: -4px;
  width: 20px;
  height: 20px;
}
.data-part .form-group .checkbox-inline input[type=checkbox]:focus,
#clearTrashDataModal .form-group .checkbox-inline input[type=checkbox]:focus,
#clearHistoryDataModal .form-group .checkbox-inline input[type=checkbox]:focus,
