.month-box * {
  margin: 0;
  padding: 0;
}
.month-box {
  width: 200px;
  font: 12px '\5B8B\4F53';
  z-index: 99999999;
  *margin: -2px 0 0 -2px;
  *overflow: hidden;
  _margin: 0;
  _position: absolute!important;
  background-color: #fff;
}
.month-box .box-header {
  border-top: 1px solid #009F95;
  background-color: #009F95;
  position: relative;
  height: 30px;
  *width: 100%;
  z-index: 99;
}
.month-box .box-header .show-year {
  display: inline-block;
  background-color: #009F95;
  border: none;
  color: #FFF;
  height: 29px;
  line-height: 29px;
}
.month-box .box-header a {
  display: inline-block;
  width: 30px;
  height: 30px;
}
.month-box .box-header .icon-left {
  background: url("../images/icon_left.png") no-repeat center center;
  background-size: 30px 30px;
}
.month-box .box-header .icon-right {
  background: url("../images/icon_right.png") no-repeat center center;
  background-size: 30px 30px;
}
.month-box .box-table {
  width: 200px !important;
  margin: 0 !important;
  border: 1px solid #CCC;
  border-top: none;
  border-bottom: none;
  border-collapse: collapse;
  border-spacing: 0px;
  font-size: 12px;
  z-index: 99999999;
  background-color: #FFF;
}
.month-box .box-table td {
  border: none;
  width: 31px;
  height: 28px!important;
  line-height: 28px;
  text-align: center;
  cursor: pointer;
  font-size: 12px;
  color: #00625A;
  background-color: #FFF;
}
.month-box .box-table td:hover {
  background-color: #00C1B3;
  color: #fff;
}
.month-box .box-table .month-click {
  background-color: #009F95!important;
  color: #fff!important;
}
.month-box .box-table .month-void {
  color: #00E8D7!important;
}
.month-box .box-bottom {
  border: 1px solid #CCC;
  border-top: none;
  position: relative;
  height: 31px;
  line-height: 20px;
  padding: 5px;
  font-size: 12px;
}
.month-box .box-bottom .month-btn {
  position: absolute;
  right: 5px;
  top: 5px;
  border-right: 1px solid #CCC;
}
.month-box .box-bottom .month-btn a {
  border: 1px solid #CCC;
  border-right: none;
  background-color: #FFF;
  float: left;
  height: 20px;
  padding: 0 6px;
  color: #00625A;
}
.month-box .box-bottom .month-btn a:hover {
  background-color: #F6F6F6;
  color: #00625A;
  text-decoration: none;
  cursor: pointer;
}
