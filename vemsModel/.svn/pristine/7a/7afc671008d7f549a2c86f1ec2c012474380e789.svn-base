 <script>
      $(function(){
        $(".close-modal").on("click",function(){
            var index = parent.layer.getFrameIndex(window.name); //先得到当前iframe层的索引
            parent.layer.close(index);
        })

          //点击查看大图
        $(document).on('click', '.look-plateImg', function(e) {
            $('#look-bigPlate-modal').modal('show');

        });

      })
 </script>
