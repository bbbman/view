    <script type="text/javascript">
    $(function(){
        if (!$.browser.webkit) {
            $.mCustomScrollbar.defaults.scrollButtons.enable=false; //滑块的上下的button是否显示
            $.mCustomScrollbar.defaults.axis="y"; //滑块的放行 X轴或Y轴
             $(".main-sidebar").mCustomScrollbar({theme:"dark"});
      }


       //弹出新增一体机服务窗口
        $("#setChargeArea").click(function(){
            layer.open({
                type: 2,
                title: '设置计费区域',
                maxmin: true,
                shadeClose: false, //点击遮罩关闭层
                area : ['1000px' , '85%'],
                offset : ['30px'],
                content: ['modal/set-channel-modal.html?way=charge']
            });
             var wrap =  $(".layui-layer-content ");
             browser_ = /iphone|ipad/i.test(navigator.userAgent);
             if(browser_ == true) {
                 wrap.addClass("iframe-wrap");
             }
             return false;
        }); 

         $("#setStatisticsArea").click(function(){
            layer.open({
                type: 2,
                title: '设置统计区域',
                maxmin: true,
                shadeClose: false, //点击遮罩关闭层
                area : ['1000px' , '85%'],
                offset : ['30px'],
                content: ['modal/set-channel-modal.html?way=statistics']
            });
             var wrap =  $(".layui-layer-content ");
             browser_ = /iphone|ipad/i.test(navigator.userAgent);
             if(browser_ == true) {
                 wrap.addClass("iframe-wrap");
             }
             return false;
        });

      $("#add-channel-modal").click(function() {
        layer.open({
            type: 2,
            title: '新增通道',
            maxmin: true,
            shadeClose: false, //点击遮罩关闭层
            area: ['600px', '78%'],
            offset: ['30px'],
            content: ['modal/add-channel-modal.html']
        });
        var wrap = $(".layui-layer-content ");
        browser_ = /iphone|ipad/i.test(navigator.userAgent);
        if (browser_ == true) {
            wrap.addClass("iframe-wrap");
        }
        return false;
    });

        $(document).on("click",".edit-channel-modal",function(){
            layer.open({
                type: 2,
                title: '修改通道',
                maxmin: true,
                shadeClose: false, //点击遮罩关闭层
                area: ['600px', '78%'],
                offset: ['30px'],
                content: ['modal/edit-channel-modal.html']
            });
            var wrap = $(".layui-layer-content ");
            browser_ = /iphone|ipad/i.test(navigator.userAgent);
            if (browser_ == true) {
                wrap.addClass("iframe-wrap");
            }
            return false;
        });

      $(document).on("click",".bind-chargeArea-modal",function(){
            
            layer.open({
                type: 2,
                title: '绑定计费区域',
                maxmin: true,
                shadeClose: false, //点击遮罩关闭层
                area: ['600px', '78%'],
                offset: ['30px'],
                content: ['modal/bind-chargeArea-modal.html']
            });
            var wrap = $(".layui-layer-content ");
            browser_ = /iphone|ipad/i.test(navigator.userAgent);
            if (browser_ == true) {
                wrap.addClass("iframe-wrap");
            }
            return false;
        });

        $(document).on("click",".channelDetail",function(){
              layer.open({
                  type: 2,
                  title: '修改通道',
                  maxmin: true,
                  shadeClose: false, //点击遮罩关闭层
                  area: ['600px', '78%'],
                  offset: ['30px'],
                  content: ['modal/channelDetail.html']
              });
              var wrap = $(".layui-layer-content ");
              browser_ = /iphone|ipad/i.test(navigator.userAgent);
              if (browser_ == true) {
                  wrap.addClass("iframe-wrap");
              }
              return false;
        });


        $('#open-channelEvent').on('click',function(){
            layer.open({
                type: 2,
                title: '设置通道触发事件',
                maxmin: true,
                shadeClose: false, //点击遮罩关闭层
                area: ['1000px', '85%'],
                offset: ['30px'],
                content: ['modal/setChannelEvent.html']
            });
            var wrap = $(".layui-layer-content ");
            browser_ = /iphone|ipad/i.test(navigator.userAgent);
            if (browser_ == true) {
                wrap.addClass("iframe-wrap");
            }
            return false;
        })

    });

    </script>
