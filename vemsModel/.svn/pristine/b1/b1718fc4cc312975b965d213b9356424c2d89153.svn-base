            maxmin: true,
            shadeClose: false, //点击遮罩关闭层
            area : ['50%' , '40%'],
            offset : ['30px'],
            content: ['modal/backList_setting.html']
        });
    })


    $(".ake-third-menu").on('click',function(){
        var src = $(this).attr('href');
        parent.$("#ake-iframe").attr("src",src);
    })

