    <script type="text/javascript">
    $(function(){
        if (!$.browser.webkit) {
            $.mCustomScrollbar.defaults.scrollButtons.enable=false; //滑块的上下的button是否显示
            $.mCustomScrollbar.defaults.axis="y"; //滑块的放行 X轴或Y轴
             $(".main-sidebar").mCustomScrollbar({theme:"dark"});
      }
      //绑定时间区间控件
      $('#leaveTime').binDaterangepicker({
                isSingle: false,
                format: 'YYYY-MM-DD HH:mm',
                isTimePicker: true,
                position:'left'
            });

        //绑定时间区间控件
      $('#approchTime').binDaterangepicker({
                isSingle: false,
                format: 'YYYY-MM-DD HH:mm',
                isTimePicker: true,
                position:'left'
            });

       //车牌校正大图片的显示与变化
       $(document).on('mouseover','.discern-result-list img',function(e){
          var $this=$(e.target);
          var $bigImgBox=$('.big-plateImg-box');
          var $bigImg=$('.big-plateImg-box img');
          var bigImgSrc=$bigImg.attr('src');
              $this.on('click',function(){
                 bigImgSrc=$this.attr('src');
                 $bigImg.attr('src',bigImgSrc);
                // $('.big-plateImg-box').removeClass('hidden');
              });
              if(!$bigImgBox.hasClass('hidden')){
                   $bigImg.attr('src',$this.attr('src'));
                    $this.on('mouseout',function(){
                         $bigImg.attr('src',bigImgSrc);
                    });
               }
           });
       //点击查看大图
       $(document).on('click','.look-plsteImg',function(e){
             $('#look-bigPlate-modal').modal('show');

       });

       //match-approch-modal显示车牌大图
       $(document).on('mouseover','.match-approch-list .approch-img img',function(e){
          var $this=$(e.target);
          var bigImgStr='<div class="bigImg"><img src="'+$this.attr('src')+'" class="img-thumbnail " style=""><div>';
              $this.parent().append(bigImgStr);
              $this.on('mouseout',function(){
                 $(this).siblings('.bigImg').remove();
              });
       });

        //match-approch-modal选择匹配进场的记录
        $(document).on('click','.match-approch-list .match-approch-item',function(e){
            var $this=$(e.currentTarget);
            var $matchBox=$('.match-approch-box');
                $matchBox.find('img').attr('src',$this.find('img').attr('src'));
                $matchBox.find('.plate-num').text($this.find('.item-detail .plate-num').text());
                $matchBox.find('.leave-time').text($this.find('.item-detail .leave-time').text());
        });


        $(document).on("click",".showMatchingDetail",function(){
            layer.open({
                type: 2,
                title: '查看匹配',
                maxmin: true,
                shadeClose: false, //点击遮罩关闭层
                area : ['500px' , '80%'],
                offset : ['30px'],
                content: ['../modal/matchingDetail-modal.html']
            });
            return false;
        });

        $(document).on("click",".open-match-approch-modal",function(){
            layer.open({
                type: 2,
                title: '查看匹配',
                maxmin: true,
                shadeClose: false, //点击遮罩关闭层
                area : ['80%' , '80%'],
                offset : ['30px'],
                content: ['../modal/matchingEnter.html']
            });
            var wrap =  $(".layui-layer-content ");
            browser_ = /iphone|ipad/i.test(navigator.userAgent);
            if(browser_ == true) {
                wrap.addClass("iframe-wrap");
            }
            return false;
        });

        $(document).on("click", ".open-leavecar-detail-modal", function() {
            layer.open({
                type: 2,
                title: '历史出场车辆详情',
                maxmin: true,
                shadeClose: false, //点击遮罩关闭层
                area: ['700px', '80%'],
                offset: ['30px'],
                content: ['../modal/approchleave-detail-modal.html']
            });
            var wrap =  $(".layui-layer-content ");
            browser_ = /iphone|ipad/i.test(navigator.userAgent);
            if(browser_ == true) {
                wrap.addClass("iframe-wrap");
            }
            return false;
        })
    });
        
    </script>
</body>

